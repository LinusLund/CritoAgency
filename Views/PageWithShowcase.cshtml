@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.PageWithShowcase>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "_layout.cshtml";
	ViewData["PageTitle"] = @Model.Value("pageTitle");
	ViewData["PageDescription"] = @Model.Value("pageDescription");
	ViewData["PageKeywords"] = @Model.Value("pageKeywords");
}
<!--header-->
<header>

    @await Html.PartialAsync("_ShowcaseSection")

</header>
<main>
    @await Html.PartialAsync("_presentationLogos")
    @await Html.PartialAsync("_ServicesSection")
    @await Html.PartialAsync("_ChooseUsPartial")
    @await Html.PartialAsync("_globalProjectsPartial")
    @await Html.PartialAsync("_teamPartial")
    @await Html.PartialAsync("_TestimonyPartial")
    @await Html.PartialAsync("_articleSectionRowPartial")

</main>